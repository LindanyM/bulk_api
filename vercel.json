{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        },
        { "src": "/api-docs", "dest": "/api/index.js" },
        { "src": "/.*", "dest": "/api/index.js" }
    ]      
  }